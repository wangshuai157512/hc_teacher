 @import "../common/common.wxss";

Page {
  width: 100%;
  height: 100%;
  font-size: 38rpx;
  background:rgba(255,255,255,1);
}
.order-content-box{
  box-sizing: border-box;
  padding:0 16px;
  display: flex;
}
.order-content{
  width: 100%;
  box-sizing: border-box;
  padding: 15px 0;
  display: flex;
  border-bottom: 1px solid #F1F5F9;
}
.order-title-box{
  padding-right: 41px;
  white-space: nowrap;
}
.order-content-text{
  flex: 1;
  text-align: right;
  display: inline-flex;
  justify-content: flex-end;
  align-items: center;

}
.order-content-text .arrow {
  width:14px;
  height: 14px;
}
.submit-order{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  background: linear-gradient(to right,#FFBB33, #FF8800);
  height: 50px;
  line-height: 50px;
  text-align: center;
}
.order-content-text2{
  flex: 1;
  text-align: right;
}
