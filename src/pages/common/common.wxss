.shadow {
  width: 100%;
  height: 100%;
  background: #000000;
  position: fixed;
  z-index: 19;
  opacity: 0.4;
  left: 0;
}

.dialog_my {
  width: 285px;
  position: absolute;
  z-index: 99;
  top: 76px;
  left: 50%;
  margin-left: -142.5px;
  border-radius: 8px;
  text-align: center;
}

.dialog_png {
  width: 285px;
}

.close_img_small {
  width: 32px;
  height: 32px;
  margin-top: 20px;
}

.dialog_cont {
  width:246px;
  position:absolute;
  top:120px;
  margin:0 auto;
  left:19.5px;
  display:flex;
  flex-direction:column;
  justify-content:center;
  text-align:center;
  align-items:center;
}

.cont_get {
  font-size: 16px;
  color: #60656C;
