}

.new_dialog_link {
  width: 221px;
  height: 44px;
  border-radius: 22px;
  color: #fff;
  background: linear-gradient(to right, #29CC44 , #00B359);
  font-size: 16px;
  line-height: 44px;
  box-shadow:0px 6px 12px 0px rgba(0,212,103,0.35);
}

.new_dialog_link text {
  font-size: 16px;
  line-height: 44px;
  color: #fff;
}

.dialog_link {
  width: 221px;
  height: 44px;
  border-radius: 22px;
  color: #fff;
  background: linear-gradient(to right, #29CC44 , #00B359);
  font-size: 16px;
  line-height: 44px;
  position: absolute;
  opacity: 0;
  bottom: 0;
}


